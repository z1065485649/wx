.login {
  background: #F0F6FF;
  .header {
    width: 100%;
    height: 80px;
    line-height: 80px;
    background: #fff;
    .logo {
      float: left;
      margin: 0 0 0 28px;
      .icon-logo {
        display: inline-block;
        width: 120px;
        height: 23px;
        background: url(../../static/image/icon_Logo.png) no-repeat;
        vertical-align: middle;
        margin: -5px 0 0 0;
      }
      .blue {
        font-size: 20px;
        font-weight: bold;
        color: #56A4FF;
      }
      .gray {
        margin: 0 5px;
        font-size: 22px;
        color: #D8D8D8;
      }
    }
  }
  .login-form {
    width: 100%;
    height: 580px;
    background: url(../../static/image/index-bg.jpg) no-repeat scroll center center / cover;
    clear: both;
    .main {
      width: 1100px;
      margin: 0 auto;
      position: relative;
      .bg-font {
        float: left;
        color: #fff;
        animation: lefteaseinAnimate 1s ease 1;
        animation-fill-mode: forwards;
        .font-42 {
          font-size: 42px;
          padding-top: 230px;
        }
        .font-36 {
          font-size: 36px;
          margin-left: 50px;
        }
      }
      .login-box {
        width: 416px;
        height: 412px;
        margin-top: 60px;
        background-color: rgba(255, 255, 255, 0.8);
        border-radius: 8px;
        float: right;
        position: relative;
        .nav ul li {
          height: 86px;
          line-height: 86px;
          width: 49.5%;
          color: #56A4FF;
          font-size: 20px;
          display: inline-block;
          text-align: center;
          transition: opacity 1s linear;
        }
        .form {
          position: relative;
          width: 100%;
          height: 370px;
          text-align: center;
          padding: 10px 0;
          .form-input {
            width: 386px;
            height: 59px;
            margin: 20px auto;
            background: #fff;
            border: 1px solid #fff;
            border-radius: 4px;
            position: relative;
            .icon-name {
              position: absolute;
              top: 12px;
              left: 10px;
              display: inline-block;
              width: 24px;
              height: 30px;
              background: url(../../static/image/icon-name.png) no-repeat;
            }
            .icon-psw {
              position: absolute;
              top: 13px;
              left: 10px;
              display: inline-block;
              width: 24px;
              height: 28px;
              background: url(../../static/image/icon-psw.png) no-repeat;
            }
            input {
              width: 100%;
              height: 59px;
              border: none;
              text-indent: 50px;
              border-radius: 4px;
            }
          }
          .submit {
            width: 386px;
            height: 59px;
            margin: 40px auto;
            background: #56A4FF;
            color: #fff;
            border-radius: 4px;
            border: none;
            font-size: 22px;
            cursor: pointer;
            transition: all 0.5s linear;
            &:hover {
              background: #418EE7;
            }
          }
        }
      }
    }
  }
  .footer {
    width: 100%;
    height: 52px;
    line-height: 52px;
    padding: 0 52px;
    background: #fff;
    color: #9B9B9B;
    text-align: center;
  }
}